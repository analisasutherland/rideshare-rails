<h2> Passengers!</h2>
<button>
  <%= link_to( 'New Passenger', new_passenger_path(@passenger)) %>
</button>
<div>
  <%= paginate @passengers %>
</div>
<ul class="Trip_List">
  <% @passengers.each do |passenger| %>
  <li>
    <%= link_to(passenger.id, passenger_path(passenger)) %>

    <%= link_to(passenger.name, passenger_path(passenger)) %>
  </li>
  <% end %>
</ul>
